{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "./src/"
    ],
    "exclude": [

    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks"   : false,
    "monospaceLinks": false,
    "default"       : {
      "outputSourceFiles" : true
    },
    "applicationName": "air-stream-local-mim",
    "disqus": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "linenums"      : true
  },
  "opts": {
    "encoding"      : "utf8",
    "recurse"       : true,
    "private"       : false,
    "lenient"       : true,
    "destination"   : "./docs"
  }
}